<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Exam Schedule</title>
	</head>
<body class="wow fadeIn">
	<!-- header start -->	
	<header class="navbar dash-header" id="navbar">
		<div class="navbar-btn">
			<a class="nav-open-nav" id="open-nav" onclick="openSideBar()" title="Click to open"> 
				<i class="fa fa-bars"></i>
			</a>
			<a class="nav-cls-btn" id="close-nav" onclick="closeSideBar()" title="click to close"> 
				 <i class="fa fa-bars"></i>
			</a>
		</div>
		<div class="brand">	
			<a  href="javascript:void(0)" class="dash-logo" id="logo" title="Quiztech">QuizTech</a>
		</div>
	</header>
	<!-- header end -->
	
	<!-- sidebar start -->
	<aside class="side-nav wow slideInLeft" id="sidenav">
		<ul class="categories">
			<li>
				<a href="javascript:void(0)" title="Dashboard">
					<i class="fa fa-dashboard"></i><span>Dashboard</span></a>
			</li>
			<li class="drop">
				<a href="javascript:void(0)" title="Question Bank">
					<i class="fa  fa-pencil-square"></i><span>Question Bank</span></a>
				<ul class="side-nav-dropdown">
					<li><a href="javascript:void(0)" title="Subjects">
						<i class="fa fa-book sub-icon"></i>Subjects</a></li>
					<li><a href="javascript:void(0)" title="Topics">
						<i class="fa fa-file sub-icon"></i>Topics</a></li>
					<li><a href="javascript:void(0)" title="Question Type">
						<i class="fa fa-briefcase sub-icon"></i>Question Type</a></li>
					<li><a href="javascript:void(0)" title="Objective Question">
						<i class="fa fa-file-text sub-icon"></i>Objective Question</a></li>
			   </ul>
			</li>
			<li>
				<a href="javascript:void(0)" title="Exam Creation">
					<i class="fa fa-pencil-square-o"></i><span>Exam Creation</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" title="Exam Scheduler" class="active-dashlink">
				   <i class="fa fa-calendar"></i><span>Exam Scheduler</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" title="Candidate Data">
				   	<i class="fa fa-users"></i><span>Candidate Data</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" title="Candidate Result">
				    <i class="fa fa-check-square"></i><span>Candidate Result</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" class="side-link" title="Logout">
				   <i class="fa fa-sign-out side-icon"></i><span>Logout</span></a>
		   </li>
		</ul>
		<div class="nav-btn">
			<a class="open-nav" id="open" onclick="openSideBar()" title="Click to open"> 
				<i class="fa fa-angle-double-right open-icon"></i>
			</a>
			<a class="cls-btn" id="close" onclick="closeSideBar()" title="click to close"> 
				 <i class="fa fa-angle-double-left cls-icon"></i>
			</a>
		</div>	
	</aside>
	<!-- sidebar end -->	
	
	<!--main content start-->
	<main id="main">
		<div class="dash-content">
			<div class="page-indicator">
				<a class="indicate-arrow" href="javascript:void(0)" title="Dashboard"><i class="fa fa-home"></i></a>
				<span>Exam Schedule</span>
			</div>
			<div class="subject-container">
				<div class="button-input-container">
					<div class="button-section">
						<button class="btn add-new" data-toggle="modal" data-target="#schedule-Modal"><i class="fa fa-plus-circle "></i>New</button>
					</div>
				</div>
				
				<div class="subject-table">
					<table class="table table-responsive">
						<thead class="t-head">
							<tr>
								<th><input type="checkbox" name="subject-check"></th>
								<th>Exam name </th>
								<th>Start Date </th>
								<th>End Date </th>
								<th>Start time</th>
								<th>End time </th>
							</tr>
						</thead>
						<tbody class="t-body">
							<tr class="t-row">
								<td><input type="checkbox" name="subject-check"></<!-- td>
								<td><span>DBMS </span></td>
								<td><span>2-5-2020 </span></td>
								<td><span>2-5-2020 </span></td>
								<td><span>11:00am</span></td>
								<td><span>12:00am</span></td> -->
							<% @sch.each do |schedule| %>
							
							<tr><td><%= schedule.examname %></td>
							<td><%= schedule.startdate %></td>
							<td><%= schedulen.enddate%></td></tr>
							<td><%= schedulen.starttime%></td></tr>
							<td><%= schedulen.endtime%></td></tr>
							<%end%>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</main>
	<!--main content end-->
	
	<!--New schedule Modal start-->
	<div class="modal fade" id="schedule-Modal" role="dialog">
		<div class="modal-form wow slideInDown">
			<div class="modal-content">
				<div class="modal-form-container">
					<button type="button" class="close modal-close-btn" data-dismiss="modal">&times;</button>
					<div class="modal-form-heading">
						<h3>New Exam Schedule </h3>
					</div>
					<div class="modal-form-content">
						<form method="post" action="/schedule">
							<div class="schedule-form">
								<div class="drop-col-content">
								<label>Exam name</label>
									<input type="text" name="scheduler[examname]" class="form-control">
								</div>
							</div>
							<div class="schedule-form">
								<div class="drop-col-content">
									<label>Exam start time</label>
									<input type="text" name="scheduler[starttime]" class="form-control">
								</div>
							</div>
							<div class="schedule-form">
								<div class="drop-col-content">
									<label>Exam end time</label>
									<input type="text" name="scheduler[endtime]" class="form-control">
								</div>	
							</div>
							<div class="schedule-form">
								<div class="drop-col-content">
									<label>Exam start date</label>
									<input type="text" name="scheduler[startdate" class="form-control">
								</div>
							</div>
							<div class="schedule-form">
								<div class="drop-col-content">
									<label>Exam end date</label>
									<input type="text" name="scheduler[enddate]" class="form-control">
								</div>
							</div>
						
						<div class="schedule-form-btn">
							<button type="submit" class="btn save-btn">Save</button>
							<button type="submit" class="btn cancel-btn">Cancel</button>
						</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!--New schedule Modal end-->	
			

<script src="js/index.js"></script>	
<script src="js/wow.js"></script>
<script>
   new WOW().init();
</script>	
</body>
</html>


