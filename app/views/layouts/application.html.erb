<!DOCTYPE html>
<html>
  <head>
    <title>Qtech</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <% unless controller_name=="questions"%>
      <% unless controller_name=="homes" && action_name=="dashboard" %>
      <%= render partial:  'layouts/header'%>
      <%end%>
      <%end%>
    <%= yield %>
      <% unless controller_name=="questions"%>
      <% unless controller_name=="homes" && action_name=="dashboard" %>
      <% unless controller_name=="candidates" && action_name=="candidatedata" %>
      <% unless controller_name=="schedules" && action_name=="schedule" %>
      <%= render  partial: 'layouts/footer'%>
      <%end%>
      <%end%>
      <%end%>
      <%end%>
  </body>
</html>
