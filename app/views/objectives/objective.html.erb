<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Question Bank</title>
	</head>
<body class="wow fadeIn">
	<!-- header start -->	
	<header class="navbar dash-header" id="navbar">
		<div class="navbar-btn">
			<a class="nav-open-nav" id="open-nav" onclick="openSideBar()" title="Click to open"> 
				<i class="fa fa-bars"></i>
			</a>
			<a class="nav-cls-btn" id="close-nav" onclick="closeSideBar()" title="click to close"> 
				 <i class="fa fa-bars"></i>
			</a>
		</div>
		<div class="brand">	
			<a  href="javascript:void(0)" class="dash-logo" id="logo" title="Quiztech">QuizTech</a>
		</div>
	</header>
	<!-- header end -->
	
	<!-- sidebar start -->
	<aside class="side-nav wow slideInLeft" id="sidenav">
		<ul class="categories">
			<li>
				<a href="javascript:void(0)" title="Dashboard">
					<i class="fa fa-dashboard"></i><span>Dashboard</span></a>
			</li>
			<li class="drop">
				<a href="javascript:void(0)" title="Question Bank" class="active-dashlink">
					<i class="fa  fa-pencil-square"></i><span>Question Bank</span></a>
				<ul class="side-nav-dropdown">
					<li><a href="javascript:void(0)" title="Subjects">
						<i class="fa fa-book sub-icon"></i>Subjects</a></li>
					<li><a href="javascript:void(0)" title="Topics">
						<i class="fa fa-file sub-icon"></i>Topics</a></li>
					<li><a href="javascript:void(0)" title="Question Type">
						<i class="fa fa-briefcase sub-icon"></i>Question Type</a></li>
					<li><a href="javascript:void(0)" title="Objective Question" class="sub-active-dashlink">
						<i class="fa fa-file-text sub-icon"></i>Objective Question</a></li>
			   </ul>
			</li>
			<li>
				<a href="javascript:void(0)" title="Exam Creation">
					<i class="fa fa-pencil-square-o"></i><span>Exam Creation</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" title="Exam Scheduler">
				   <i class="fa fa-calendar"></i><span>Exam Scheduler</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" title="Candidate Data">
				   	<i class="fa fa-users"></i><span>Candidate Data</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" title="Candidate Result">
				    <i class="fa fa-check-square"></i><span>Candidate Result</span></a>
		   </li>
		   <li>
			   <a href="javascript:void(0)" class="side-link" title="Logout">
				   <i class="fa fa-sign-out side-icon"></i><span>Logout</span></a>
		   </li>
		</ul>
		<div class="nav-btn">
			<a class="open-nav" id="open" onclick="openSideBar()" title="Click to open"> 
				<i class="fa fa-angle-double-right open-icon"></i>
			</a>
			<a class="cls-btn" id="close" onclick="closeSideBar()" title="click to close"> 
				 <i class="fa fa-angle-double-left cls-icon"></i>
			</a>
		</div>	
	</aside>
	<!-- sidebar end -->
	
	<!--main content start-->
	<main id="main">
		<div class="dash-content">
			<div class="page-indicator">
				<a class="indicate-arrow" href="javascript:void(0)" title="Dashboard"><i class="fa fa-home"></i></a>
				<span class="indicate-arrow">Question Bank</span>
				<span>Objective Question</span>
			</div>
			<div class="subject-container">
				<div class="button-input-container">
					<div class="button-section">
						
						<%= link_to "new", objectives_newobjective_path, class: 'btn add-new' %>
						
					</div>
					<div class="search">
						<div>
							<input type="text" placeholder="   Search " required>
						 </div>
					</div>
				</div>
				<div class="question-drop-container">
					<div class="question-drop-col">
						<div class="drop-col-content">
							<label for="select-box">Subjects</label>
							<%=collection_select( "topic" ,"subject_id", Subject.all ,:id, :name, {:multiple => false })%> 			
						</div>
					</div>
					<div class="question-drop-col">
						<div class="drop-col-content">
							<label for="select-box">Topic</label>
							<%=collection_select( "topic" ,"topic_id", Topic.all ,:id, :name, {:multiple => false })%> 		
						</div>
					</div>
					
				</div>
				<div class="objective-total-container">
					<div class="objective-total-col">
						<div class="objective-col-content">
							<div class="objective-col-img">
								<img src="img/total.png" alt="book" width="100" height="100" class="img-responsive">
							</div>
							<div class="objective-total-col-content">
								<p>Total Question :</p>
								<span>1</span>
							</div>	
						</div>
					</div>
					<div class="objective-total-col">
						<div class="objective-col-content">
							<div class="objective-col-img">
								<img src="img/active.png" alt="book" width="60" height="60" class="img-responsive">
							</div>
							<div class="objective-total-col-content">
								<p>Active Question :</p>
								<span>1</span>
							</div> 		
						</div>
					</div>
					<div class="objective-total-col">
						<div class="objective-col-content">
							<div class="objective-col-img">
								<img src="img/deactive.png" alt="book" width="50" height="50" class="img-responsive">
							</div>
							<div class="objective-total-col-content">
								<p>Deactive Question :</p>
								<span>0</span>
							</div>		
						</div>
					</div>
				</div>
				<div class="subject-table">
					<table class="table table-responsive">
						<thead class="t-head">
							<tr>
								<th><input type="checkbox" name="subject-check"></th>
								<th>Question</th>
								<th>Subject Name</th>
								<th>Topic Name</th>
								<th>Question Status</th>
								<th>Difficulty Level</th>
								<th>Marks</th>
								<th>Edit</th>
								<th>View</th>
							</tr>
						</thead>
						<tbody class="t-body">
							<tr class="t-row">
								<td><input type="checkbox" name="subject-check"></td>
								<td><span>What is an array?</span></td>
								<td><span>C++</span></td>
								<td><span>Array</span></td>
								<td><span>Active</span></td>
								<td><span>Medium</span></td>
								<td><span>1</span></td>
								</a></td>
								<td><a href="javascript:void(0)" title="view question" class="view-icon">
								</a></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</main>
	<!--main content end-->
<script src="js/index.js"></script>	
<script src="js/wow.js"></script>
<script>
   new WOW().init();
</script>	
</body>
</html>


